<h1>Photos</h1>

<%= form_for [:prive, :site, @photo], :multipart => true do |f| %>
	<%= render "/shared/error_messages", :target => @photo %>
	<div>
		<%= f.file_field :picture %>		
	</div>
	<div>
		<%= f.submit 'Ajouter', :class => 'btn' %>
		<%= link_to 'annuler', prive_site_photos_path %>
	</div>
	
<% end %>